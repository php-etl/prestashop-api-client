{
    "name": "php-etl/prestashop-api-client",
    "description": "Prestashop API client for ETL Pipeline",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "symfony/serializer": "^6.2",
        "prestashop/prestashop-webservice-lib": "dev-feature/better-error-handling"
    },
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Prestashop\\ApiClient\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    },
    "repositories": {
        "php-etl/prestashop-plugin": {
            "url": "https://github.com/php-etl/prestashop-webservice-lib",
            "type": "vcs"
        }
    }
}
